{
  "compilerOptions": {
    "target": "ES5",
    "lib": [
      "DOM",
      "ES2015"
    ],
    "rootDir": "./tmp",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "declarationDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitAny": false,
    "ignoreDeprecations": "5.0",
    "noEmit": true,
  },
  "skipLibCheck": true,
  "files": [
    "type/gc.d.ts"
  ],
  "include": [
    "tmp/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
